@model Service
@{
    ViewData["Title"] = "Create";
}

<div class="mt-3">
    <form method="post" enctype="multipart/form-data">
      <button class="btn btn-primary mt-4" style="width:200px; height:20px; color:#fff" type="submit" >Create</button>
        <div>
            <label>Icon</label>
            <input asp-for="Icon" class="form-control" / placeholder="Icon">
            <span asp-validation-for="Icon" class="text-danger"></span>
        </div>
        <div>
            <label>Title</label>
            <input asp-for="Title" class="form-control" / placeholder="Title">
            <span asp-validation-for="Title" class="text-danger"></span>
        </div>
        <div>
            <label>Subtitle</label>
            <input asp-for="Subtitle" class="form-control" / placeholder="Subtitle">
            <span asp-validation-for="Subtitle" class="text-danger"></span>
        </div>
        <div>
            <label>Sentence</label>
            <input asp-for="Sentence" class="form-control" / placeholder="Sentence">
            <span asp-validation-for="Sentence" class="text-danger"></span>
        </div>
       
    </form>
</div>

